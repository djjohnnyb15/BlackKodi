			<checksum>https://raw.githubusercontent.com/djjohnnyb15/BlackKodi/master/addons.xml.md5</checksum>
			<datadir zip="true">https://raw.githubusercontent.com/djjohnnyb15/BlackKodi/master/zips</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary lang="en">Black Kodi Wizard-Repo</summary>
    <description lang="en">Black Kodi Wizard Easy Install for kodi.</description>
		<platform>all</platform>
	</extension>
</addon>
